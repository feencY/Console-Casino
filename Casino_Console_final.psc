// Algoritmo Principal del Juego BlackJack en forma de Subproceso 
SubProceso dinero<-BlackJack(plata Por Referencia, apuesta Por Referencia)
	definir baraja, i, mazo, puntos como Entero
	Dimension baraja[53];
	mazo=1
	puntos=0
	//Iniciar la baraja
	Para i<-1 hasta 52 Con Paso 1 Hacer
		baraja[i]=1
	FinPara
	
	CrearBaraja(baraja)
	MezclarBaraja(baraja)
	TiradaJugador(baraja,mazo,puntos,apuesta,plata)
	Si puntos<22 Entonces
		TiradaCrupier(baraja,mazo,puntos,PuntosCrupier,plata,apuesta)
	FinSi
	dinero<-plata
	Escribir "Tu plata restante es: ",plata
FinSubProceso

//Funcion que crea la baraja hasta la carta 13
Funcion CrearBaraja(mazo)
	Definir i, cuenta Como Entero
	cuenta=1
	Escribir "Turno del jugador"
	Esperar 2 Segundos
	para i<-1 hasta 52 con paso 1 Hacer
		mazo[i]=cuenta
		cuenta=cuenta + 1
		Si cuenta=14 Entonces
			cuenta=1
		FinSi
	FinPara
FinFuncion

// Se mezclan las cartas de la baraja ya creada guardandolas en distintos auxiliares con funcion Azar
Funcion MezclarBaraja(mazo)
	Definir PosAzar1, PosAzar2 Como Entero
	PosAzar1=0
	PosAzar2=0
	aux=0
	para i<-1 hasta 200 Con Paso 1 Hacer
		PosAzar1=azar(52) + 1
		PosAzar2=azar(52) + 1
		Mientras PosAzar2=PosAzar1 Hacer
			PosAzar2=azar(52) + 1
		FinMientras
		aux=mazo[PosAzar2]
		mazo[PosAzar1]=aux
	FinPara
FinFuncion

// En caso de ser un AS pregunta al usuario que valor desea que tome
Funcion ResultadoCarta<-TomarCarta(mazo, posicion Por Referencia)
	Definir ResultadoCarta Como Entero
	ResultadoCarta <- mazo[posicion]
	si ResultadoCarta>10 Entonces
		ResultadoCarta<-10
	FinSi
	Si ResultadoCarta=1 Entonces
		Escribir "Sacaste un As, cuál querés que sea su valor, 1 o 11?"
		Leer ResultadoCarta
		Mientras ResultadoCarta<>1 y ResultadoCarta<>11 Hacer
			Escribir "Ingrese un valor válido"
			leer ResultadoCarta
		FinMientras
	FinSi
	posicion=posicion+1
FinFuncion

// Turno del Jugador para jugar
Funcion TiradaJugador(mazo,posicion Por Referencia, puntuacion Por Referencia,apuesta,plata Por Referencia)
	Definir respuesta como entero
	Definir plantado Como Logico
	plantado=Falso
	i=1
	// Este mientras sirve para que tire 2 mazo automaticamente
    Mientras i < 2 Hacer
        puntuacion = puntuacion + tomarCarta(mazo, posicion)
        Escribir "Tu puntuación es " puntuacion
        i = i + 1
    FinMientras
	Mientras puntuacion<22 y plantado=Falso Hacer
		
		puntuacion=puntuacion+tomarCarta(mazo,posicion)
		Escribir "Tu puntuación es " puntuacion
		si puntuacion<22 Entonces
			Escribir "Te plantas? (1=Si/2=No)"
			leer respuesta
			Mientras respuesta<>1 y respuesta<>2 Hacer
				Escribir "Opción incorrecta, reingresa (1=Si/2=No)"
				leer respuesta
			FinMientras
			si respuesta=1 Entonces
				plantado=Verdadero
			SiNo
				plantado=Falso
			FinSi
		FinSi
	FinMientras
	Si puntuacion>21 Entonces
		Escribir "Perdiste!"
		plata=plata-apuesta
	SiNo
		Escribir "Turno del Crupier"
	FinSi
FinFuncion

// Turno del Crupier para levantar cartas y que su valor no supere 10 y elegir el valor del AS
Funcion ResultadoCarta<-TomarCartaCrupier(mazo,posicion Por Referencia,PuntosJugador Por Referencia, PuntosCrupier Por Referencia)
	definir ResultadoCarta Como Entero 
	ResultadoCarta=mazo[posicion]
	si ResultadoCarta>10 Entonces
		ResultadoCarta=10
	FinSi
	si ResultadoCarta=1 Entonces
		escribir "AS"
		si PuntosCrupier+11>21 Entonces
			ResultadoCarta=1
			escribir "El crupier elige un valor de 1"
		sino 
			ResultadoCarta=11
			escribir "El crupier elige un valor de 11"
		FinSi
	FinSi
	escribir "Carta del Crupier: ",ResultadoCarta
	posicion=posicion+1
FinFuncion

//Funcion para realizar la tirada del crupier al igual que la funcion TiradaJugador y que el crupier se plante cuando tiene un minimo de 17.
Funcion TiradaCrupier(mazo,posicion Por Referencia, PuntosJugador Por Referencia, PuntosCrupier Por Referencia,plata Por Referencia,apuesta Por Referencia)
	definir plantado Como Logico
	plantado=Falso
	mientras puntosCrupier<17 Hacer
		PuntosCrupier=PuntosCrupier+TomarCartaCrupier(mazo,posicion,puntosJugador,puntosCrupier)
		escribir "Puntuación del Crupier: ",PuntosCrupier
		esperar 1 Segundos
		Si PuntosCrupier>=PuntosJugador
			plantado=Verdadero
		FinSi
	FinMientras
	
	Si PuntosCrupier>PuntosJugador y PuntosCrupier<22 Entonces
		escribir "Perdiste!"
		plata=plata-apuesta
	sino 
		escribir "Ganaste!"
		plata=plata+apuesta
	FinSi
	Esperar Tecla
FinFuncion
	