//MOSTRAR EL CUADRADO DEL BUSCAMINAS
funcion matrizlogica(matriz,f,c,cantidadbomba,matriz2,bomba,bandera,perder) //PRIMERA FUNCION, MOSTRAR LA MATRIZ LOGICA 
	para i=1 hasta 15 con paso 1 Hacer
		si bandera[i]=0 y perder<>2 o perder=5
			j=j+1
			g=1
			si i-g=14 //SIRVE PARA MOSTRAR LAS BANDERAS ARRIBA A LA IZQUIERDA
				escribir "CANTIDAD DE BANDERAS A UTILIZAR " j 
			FinSi
		finsi
	FinPara
	si perder<>2 //CARA PARA MOSTRAR QUE VAS BIEN
		escribir "                                                                    ########################################################################"
		escribir "                                                                    ###############################  O     O  ##############################"
		escribir "                                                                    ###############################           ##############################" 
		escribir "                                                                    ###############################  -_____-  ##############################"
	SiNo //CARA DE QUE PERDISTE
		escribir "                                                                    ########################################################################"
		escribir "                                                                    ###############################  X     X  ##############################"
		escribir "                                                                    ###############################   _____   ##############################" 
		escribir "                                                                    ###############################  /     \  ##############################"
	FinSi
	escribir "                                                                    ########################################################################"	
	
	para f=1 hasta 10 con paso 1 Hacer //ESTE SISTEMA ES PARA MOSTRAR TODO LO QUE CONSISTE EL CUADRADO DEL JUEGO
		escribir "                                                                      " 
		escribir "                                                                      " sin saltar
		para c=1 hasta 10 con paso 1 Hacer
			g<-1
			e<-0
			cantidadbomba<-0
			proximidad<-0
			si matriz(f,c)=500
				para i=1 hasta 15 con paso 1 hacer //ESTO PERMITE ANALIZAR SI HAY UNA BOMBA ALREDEDOR EN CADA CUADRADO
					//TOMANDO COMO PUNTO LA BOMBA
					si c-1>0 y matriz(f,c-1)=bomba[i] 
						cantidadbomba<-cantidadbomba+1
						//DERECHA
					SiNo
						si c+1<11 y matriz(f,c+1)=bomba[i]
							cantidadbomba<-cantidadbomba+1
							//IZQUIERDA
						SiNo
							si f-1>0 y matriz(f-1,c)=bomba[i] 
								cantidadbomba<-cantidadbomba+1
								//ABAJO
							SiNo
								si f+1<11 y matriz(f+1,c)=bomba[i] 
									cantidadbomba<-cantidadbomba+1
									//ARRIBA
								SiNo
									si c-1>0 y f-1>0 y matriz(f-1,c-1)=bomba[i] 
										cantidadbomba<-cantidadbomba+1
										//ABAJO DERECHA
									SiNo
										si c-1>0 y f+1<11 y matriz(f+1,c-1)=bomba[i] 
											cantidadbomba<-cantidadbomba+1
											//ARRIBA DERECHA
										SiNo
											si c+1<11 y f+1<11 y matriz(f+1,c+1)=bomba[i] 
												cantidadbomba<-cantidadbomba+1
												//ARRIBA IZQUIERDA
											SiNo
												si c+1<11 y f-1>0 y matriz(f-1,c+1)=bomba[i] 
													cantidadbomba<-cantidadbomba+1
													//ABAJO IZQUIERDA
												FinSi
											FinSi
										FinSi
									FinSi
								FinSi
							FinSi
						FinSi
					FinSi
				finpara
				si cantidadbomba>0 //ACA SE DETERMINA SI HAY UNA BOMBA 
					proximidad<-1
					escribir  "B" cantidadbomba sin saltar
				FinSi
			finsi
			
			para i=1 hasta 15 con paso 1 Hacer //Este mecanismo es para que los primeros 9 numeros tengan el 0 y se quede perfecto el cuadrado.
				si bandera[i]<>matriz(f,c) y matriz(f,c)<=9 y matriz(f,c)<>0 y proximidad<>1 y f=1
					e<-e+1
				FinSi
				si e=15 Entonces
					escribir "0" sin saltar
				FinSi
				escribir "" sinsaltar
			fin para
			
			si matriz(f,c)=500  y proximidad<>1 //SIRVE PARA MOSTRAR LAS CASILLAS VACIAS (XX)
				escribir "XX" sin saltar
			SiNo
				si matriz(f,c)=1000  y proximidad<>1 y perder=2 // CUANDO UN NUMERO DE LA MATRIZ VALE 1000 SIGNIFICA QUE SE TRANSFORMA EN UNA BOMBA (UNICAMENTE CUANDO SE PIERDE)
					escribir "OO" sin saltar
				sino
					Para i=1 hasta 15 con paso 1 Hacer //SIRVE PARA MOSTRAR CUANDO HAYA UNA BANDERA 
						si matriz(f,c)<>0 y cantidadbomba=0 y Bandera[i]=matriz(f,c)
							Escribir "FF" sin saltar
							g<-16
						SiNo
							si Bandera[i]<>matriz(f,c) y g=15 y proximidad<>1 //SIRVE PARA MOSTRAR LOS NUMEROS "SIN DESCUBRIR"
								Escribir matriz(f,c) Sin Saltar
							FinSi
						finsi
						g<-g+1
					FinPara
				finsi
			finsi
			si matriz(f,c)=100 //CORRER UN POCO EL # PARA QUE QUEDE IGUAL EL CUADRADO
				escribir " #  " sin saltar
			SiNo
				escribir "  #  " sin saltar 
			FinSi
		FinPara
		escribir ""
	FinPara
	escribir "                                                                      " 
	escribir "                                                                    ########################################################################" 
FinFuncion
//ESTA FUNCION PERMITE ELEGIR CASILLAS ALEATORIAS CERCANA DESPUES DE QUE EL USUARIO ELIGE UNA
funcion vaciarcasillas(f,c,filazar,columnaazar,casillavacia,casillaverificada,bomba,matriz)
	para h=1 hasta 3 con paso 1 Hacer
		casilla<-0
		si h=1 Entonces
			aux<-0
		FinSi
		si f>6 y c>=6 //SISTEMA PARA QUE SE ELIGA NUMEROS RANDOM ENTRE EL 1 AL 3
			filazar[h]<-f-azar(3)
			columnaazar[h]<-c-azar(3)
		SiNo
			si  f<6 y c>=6 
				filazar[h]<-f+azar(3)
				columnaazar[h]<-c-azar(3)
			sino
				si f>=6 y c<6 
					filazar[h]<-f-azar(3)
					columnaazar[h]<-c+azar(3)
				finsi
				si f<6 y c<6 
					filazar[h]<-f+azar(3)
					columnaazar[h]<-c+azar(3)
				finsi
			finsi
		FinSi
		casillavacia[h]<-matriz(filazar[h],columnaazar[h]) //Se guarda en CASILLA VACIA Y LUEGO SE VERIFICA QUE ESTE CORRECTO EL NUMERO
		repetir
			para i=1 hasta 15 con paso 1 hacer
				si casillavacia[h]<>bomba[i] y aux<>bomba[i] y aux<>casillavacia[h]
					casilla<-casilla+1
				FinSi	
			FinPara //Si da 15 significa que funciono, y el numero es valido, es decir que no sea un 0 o uno que se repita.
			si casilla=15 y casillavacia[h]<>0
				casillaverificada[h]<-Casillavacia[h] //Casilla verificada se encarga de juntar los numeros que ya fueron utilizados. CASILLA VERIFICADA SIRVE SOLO PARA BANDERAS.
				matriz(filazar[h],columnaazar[h])<-500
				aux<-Casillaverificada[h] //Guardamos el auxiliar para que no vuelva a repetirse.
			SiNo //SI EL NUMERO VERIFICADO ESTA MAL, LO VOLVEMOS A HACER
				casilla=0
				si f>6 y c>=6 
					filazar[h]<-f-azar(3)
					columnaazar[h]<-c-azar(3)
				SiNo
					si  f<6 y c>=6 
						filazar[h]<-f+azar(3)
						columnaazar[h]<-c-azar(3)
					sino
						si f>=6 y c<6 
							filazar[h]<-f-azar(3)
							columnaazar[h]<-c+azar(3)
						finsi
						si f<6 y c<6 
							filazar[h]<-f+azar(3)
							columnaazar[h]<-c+azar(3)
						finsi
					finsi
				FinSi
				casillavacia[h]<-matriz(filazar[h],columnaazar[h])
				u=u+1
			FinSi
		hasta que casilla=15 o u>=5 //Puede pasar, si el proceso falla las 5 veces, no habra numeros aleatorios, la verdad es que no cambia nada un numero mas o menos, pero es mejor intentar que funcione 5 veces, pero a veces te puede dar 4 numeros repetidos y un 0 y mejor dejarlo asi.
	FinPara	
FinFuncion
